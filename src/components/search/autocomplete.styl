.container
  font-size: 13px
  border: 1px solid #ccc
  background-color: white
  text-align: left
  ul
    margin: 0
    padding: 0
    list-style-type: none
  a
    color: inherit;

.popOver
  position: absolute
  overflow: hidden
  width: 100%
  z-index: 2
  
.resultGroupHeader
  background-color: #eee
  font-weight: 600
  border-bottom: 1px solid #ccc
  padding: 4px 10px
  margin: 0 auto
  
.resultItem
  border-bottom: 1px solid #ccc
  padding: 4px

.resultContainer
  display: flex
  flex-direction: row
  align-items: center

.avatarContainer
  flex: 0 0 40px
  padding-right: 4px
  img
    display: block

.infoContainer
  flex: 1 0 auto

.infoPrimary
  font-weight: 600

.memberContainer
  flex: 0 0 100px
  padding-left: 4px

.seeAllResults
  display: block
  font-weight: 600
  padding: 4px
