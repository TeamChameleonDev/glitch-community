.avatarContainer
    border-radius: 5px
    position: relative
    padding: 5px
    width: 150px
    height: 150px
    &.empty
        display: none
    .projectAvatar
        position: absolute
        width: 75px
        height: 75px
        img
            border-radius: 5px
        &:nth-child(1)
            bottom: 5%
            right: 5%
        &:nth-child(2)
            top: calc(50% - 37.5px)
            left: calc(50% - 37.5px)
        &:nth-child(3)
            top: 5%
            left: 5%
